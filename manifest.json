{
    "name": "Transat Tools",
    "manifest_version": 3,
    "version": "1.0",
    "description": "Extension for lazy agents",
    "permissions": ["tabs", "activeTab", "scripting", "clipboardRead"],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "commands": {
    "show-interv": {
      "suggested_key": {
        "default": "Ctrl+Q"
      },
      "description": "Show intervention"
    }
  }
}
